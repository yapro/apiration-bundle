{
  "name": "yapro/apiration-bundle",
  "type": "library",
  "description": "Api tool",
  "keywords": ["php", "api", "tool"],
  "authors": [
    {
      "name": "Lebedenko Nikolay",
      "email": "totx@ya.ru",
      "role": "Developer",
      "homepage": "http://yapro.ru"
    }
  ],
  "require": {
    "php": ">=7.4.0",
    "yapro/marker-interfaces": "*",
    "symfony/event-dispatcher": "*",
    "symfony/http-foundation": "*",
    "symfony/http-kernel": "*",
    "symfony/translation": "*",
    "symfony/polyfill-php80": "^v1.25.0",
    "symfony/validator": "*",
    "laminas/laminas-code": "*",
    "yapro/helpers": "*",
    "symfony/serializer-pack": "*"
  },
  "require-dev": {
    "doctrine/orm": "*",
    "doctrine/dbal": "2.*",
    "symfony/phpunit-bridge": "*",
    "symfony/browser-kit": "*",
    "doctrine/annotations": "*",
    "symfony/yaml": "*",
    "symfony/routing": "*",
    "symfony/framework-bundle": "*",
    "yapro/symfony-http-test-ext": "*"
  },
  "autoload": {
    "psr-4": {
      "YaPro\\ApiRationBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "YaPro\\ApiRationBundle\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev"
}
